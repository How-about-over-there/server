{
  "family": "haot-auth-task",
  "containerDefinitions": [
    {
      "name": "auth",
      "image": "${AWS_ACCOUNT_ID}.dkr.ecr.${AWS_DEFAULT_REGION}.amazonaws.com/haot-ecr:auth-latest",
      "cpu": 256,
      "memory": 512,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 19092,
          "hostPort": 19092,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "memory": "512MB",
  "cpu": "256"
}
